{
	"name": "preferans-rating-js",
	"version": "0.0.2",
	"description": "nodejs rating for preferans",
	"engines": {
		"node": ">=8.9"
	},
	"main": "lib/rating.js",
	"scripts": {
		"test": "nyc --reporter=lcov --reporter=html mocha",
		"coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
		"analyze": "sonarjs -e .sonarlint/**,node_modules/**,coverage/**,.nyc_output/**"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cope/preferans-rating-js.git"
	},
	"keywords": [
